	height 102px
	overflow hidden
.Header 
	nav
		
		position absolute
		text-align right
		height 100%
		/* width: 100% */
		display inline-block
		right 0
	
.Header 
	nav 
		ul 
			list-style none
			margin 0
			padding 0
			
			li 
				display inline-block
				margin 0
				a 
					color #fff
					display block
					margin 1.5em 1em
					text-decoration none
					-webkit-transition 0.3s
					-moz-transition 0.3s
					-ms-transition 0.3s
					-o-transition 0.3s
					-webkit-transition 0.3s
					-moz-transition 0.3s
					-o-transition 0.3s
					-ms-transition 0.3s
					transition 0.3s
					/* height: 33px; */
					/* line-height: 3; */
					font-weight bold
					text-transform uppercase
					

