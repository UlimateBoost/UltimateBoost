
@import "normalize.styl"
@import "nib"
@import "nib/vendor"


vendor-prefixes = o ms moz webkit official


@keyframes amination
	0%
		transform rotate(0deg)
	100%
		transform rotate(360deg)
@keyframes amination2
	0%   
		transform rotate(0deg)
	100% 
		transform rotate(-360deg)
@keyframes amination3
	0%   
		transform rotate(0deg)
	100% 
		transform rotate(360deg)
@keyframes flechaAnimationP1
	0% 		
		transform rotate(0deg)
	20% 	
		transform rotate(10deg)
	40% 	
		transform rotate(0deg)
	60% 	
		transform rotate(-10deg)
	80% 	
		transform rotate(0deg)
	100% 	
		transform rotate(0deg)

@keyframes flechaAnimationP2
	from
		bottom 0
	to
		bottom 7px

