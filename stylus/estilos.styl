@import "normalize.styl"
@import "nib"
@keyframes amination
	0%
		transform rotate(0deg)
	100%
		transform rotate(360deg)
@keyframes amination2
	0%   
		transform rotate(0deg)
	100% 
		transform rotate(-360deg)
@keyframes amination3
	0%   
		transform rotate(0deg)
	100% 
		transform rotate(360deg)
@keyframes flechaAnimationP1
	0% 		
		transform rotate(0deg)
	20% 	
		transform rotate(10deg)
	40% 	
		transform rotate(0deg)
	60% 	
		transform rotate(-10deg)
	80% 	
		transform rotate(0deg)
	100% 	
		transform rotate(0deg)

@keyframes flechaAnimationP2
	from
		bottom 0
	to
		bottom 7px
		
@media screen and (max-width:1200px)
	.Titulos
			display inline-block
			top 6.5em 
			position relative
 		
.arrow
	position  absolute
	bottom  0
	/* margin  0 auto */
	width  100%
	text-align  center
	animation flechaAnimationP2 1s linear 0s infinite alternate
	figure
		width 80%
		display inline-block
		animation flechaAnimationP1 5s 
		animation-iteration-count infinite 
		margin: 0
		img
			width 150px
			opacity 0.7
